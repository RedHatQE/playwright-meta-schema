{
  "all": true,
  "include": ["dist/**/*.js", "schema/**/*.ts"],
  "exclude": [
    "dist/**/*.test.js",
    "schema/**/*.test.ts",
    "dist/**/*.test.d.ts",
    "node_modules/**",
    "coverage/**",
    "*.config.*",
    "*.test.*"
  ],
  "reporter": ["text", "text-summary", "html", "lcov", "json", "json-summary"],
  "reports-dir": "coverage",
  "temp-directory": "coverage/.nyc_output",
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 75,
  "statements": 80,
  "clean": true,
  "skip-full": false
}
